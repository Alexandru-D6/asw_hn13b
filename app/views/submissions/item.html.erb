<html lang="en" op="item">
   <body>
      <center>
         <table id="hnmain" border="0" cellpadding="0" cellspacing="0" width="85%" bgcolor="#f6f6ef">
            <tbody>
               <%= notice %>
            </tbody>
         </table>
         <table id="hnmain" border="0" cellpadding="0" cellspacing="0" width="85%" bgcolor="#f6f6ef">

            <tr id="pagespace" title="" style="height:10px"></tr>
            <tr>
               <td>
                  <table class="fatitem" border="0">
                     <tbody>
                        
                        
                        <% @submission.each do |submission| %>
                        <tr class="athing">
                           <td align="right" valign="top" class="title"><span class="rank"></span></td>
                           <td valign="top" class="votelinks">
                          <center>
                              <% if user_signed_in? && (current_user.LikedSubmissions.detect{|e| e == submission.id.to_s}.nil?) %>
                                 <%= link_to "▲", upvote_submission_path(submission), method: :put%>
                              <%else%>
                                 <%if !user_signed_in? %>
                                    <%= link_to "▲", upvote_submission_path(submission), method: :put%>
                                 <%end%>
                              <%end%>
                          </center>
                        </td>
                           <td class="title">
                          <a href=<%=submission.url%> class="titlelink"><%= submission.title %></a>
                          <span class="sitebit comhead">
                             (<a href=<%=submission.url%>>
                               <%link = "hola"%>
                               <script type="text/javascript" >
                                 link=submission.url.substring(0,10);
                               </script>
                               <span class="sitestr"><%=link%></span>
                             </a>)
                           </span>
                       </td>
                        </tr>
                        <tr>
                           
                        <td colspan="2"></td>
                         <td class="subtext">
                          <span class="score" id="score_30782735"><%=submission.UpVotes%></span> 
                          by 
                          
                          <%= link_to submission.author_username, "/user?id="+submission.author_username%>
                          
                          <span class="age" title="2022-03-23T20:11:49">
                            <a href=<%= "item?id="+submission.id.to_s%>><%=distance_of_time_in_words(submission.created_at, Time.new)%> ago</a>
                          </span> 
                          <span id="unv_30782735"></span> | 
                          
                           <% if user_signed_in? && !(current_user.LikedSubmissions.detect{|e| e == submission.id.to_s}.nil?) %>
                             <%= link_to "unvote", unvote_submission_path(submission), method: :put%> |
                           <%end%>
                          
                          <a href="news" class="clicky">hide</a> | 
                          <a href="news">&nbsp;comments</a>              
                        </td>
                        </tr>
                        <% end %>
                        
                        
                        <tr style="height:10px"></tr>
                        <tr>
                           <td colspan="2"></td>
                           <td>
                              <form method="post" action="comment"><input type="hidden" name="parent" value="30961724"><input type="hidden" name="goto" value="item?id=30961724"><input type="hidden" name="hmac" value="34a6069ddf5417fd89bda95e6044ea5e56d11059"><textarea name="text" rows="8" cols="80"></textarea>
                                 <br><br><input type="submit" value="add comment">
                              </form>
                           </td>
                        </tr>
                     </tbody>
                  </table>
                  <br><br>
               </td>
            </tr>
            
            
            
            
            
            
            <tr>
               <td>
